{
  "name": "azahari/laravel-serial-pattern",
  "description": "A Laravel 12 Composer package for configurable serial number generation with optional audit logging and model integration.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Azahari Zaman",
      "email": "azaharizaman@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Azahari\\SerialPattern\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Azahari\\SerialPattern\\SerialPatternServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}